{
  "name": "svelte-maplibre",
  "version": "1.2.3",
  "description": "Svelte bindings for MapLibre",
  "author": {
    "name": "Daniel Imfeld",
    "url": "https://imfeld.dev"
  },
  "repository": {
    "url": "https://github.com/dimfeld/svelte-maplibre"
  },
  "homepage": "https://svelte-maplibre.imfeld.dev",
  "license": "MIT",
  "keywords": [
    "gis",
    "map",
    "maplibre",
    "svelte"
  ],
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build && npm run package",
    "preview": "vite preview",
    "package": "svelte-kit sync && svelte-package && publint",
    "prepare": "husky && npm run package",
    "test": "playwright test",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "changeset": "changeset",
    "generate-changelog": "source ghtoken; GITHUB_TOKEN=${GITHUB_TOKEN} changeset version",
    "lint": "prettier --plugin-search-dir . --check src && eslint .",
    "format": "prettier --plugin-search-dir . --write src"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "svelte": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./*.css": {
      "default": "./dist/*.css"
    },
    "./*.svelte": {
      "types": "./dist/*.d.ts",
      "svelte": "./dist/*.svelte",
      "default": "./dist/*.svelte"
    },
    "./context.svelte.js": {
      "types": "./dist/context.svelte.d.ts",
      "default": "./dist/context.svelte.js"
    },
    "./expressions.js": {
      "types": "./dist/expressions.d.ts",
      "default": "./dist/expressions.js"
    },
    "./filters.js": {
      "types": "./dist/filters.d.ts",
      "default": "./dist/filters.js"
    },
    "./source.js": {
      "types": "./dist/source.d.ts",
      "default": "./dist/source.js"
    },
    "./types.js": {
      "types": "./dist/types.d.ts",
      "default": "./dist/types.js"
    }
  },
  "svelte": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "dependencies": {
    "d3-geo": "^3.1.0",
    "dequal": "^2.0.3",
    "just-compare": "^2.3.0",
    "maplibre-gl": "^4.0.0 || ^5.0.1",
    "pmtiles": "^3.0.3"
  },
  "peerDependencies": {
    "@deck.gl/core": "^9",
    "@deck.gl/layers": "^9",
    "@deck.gl/mapbox": "^9",
    "svelte": "^5.0.0"
  },
  "devDependencies": {
    "@changesets/changelog-github": "^0.5.1",
    "@changesets/cli": "^2.29.7",
    "@deck.gl/core": "~9.1.15",
    "@deck.gl/layers": "~9.1.15",
    "@deck.gl/mapbox": "~9.1.15",
    "@mapbox/mapbox-gl-draw": "^1.5.0",
    "@playwright/test": "^1.56.1",
    "@skeletonlabs/skeleton": "^2.11.0",
    "@skeletonlabs/tw-plugin": "^0.4.1",
    "@sveltejs/adapter-vercel": "^5.10.3",
    "@sveltejs/kit": "^2.48.0",
    "@sveltejs/package": "^2.5.4",
    "@sveltejs/vite-plugin-svelte": "^4.0.4",
    "@types/d3-color": "^3.1.3",
    "@types/d3-geo": "^3.1.0",
    "@types/geojson": "^7946.0.16",
    "@types/mapbox__mapbox-gl-draw": "^1.4.9",
    "@types/node": "^22.18.12",
    "@typescript-eslint/eslint-plugin": "^8.46.2",
    "@typescript-eslint/parser": "^8.46.2",
    "autoprefixer": "^10.4.21",
    "d3-color": "^3.1.0",
    "dedent": "^1.7.0",
    "eslint": "^9.38.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.15.0",
    "highlight.js": "^11.11.1",
    "highlightjs-svelte": "^1.0.6",
    "husky": "^9.1.7",
    "just-clamp": "^4.2.0",
    "lint-staged": "^15.5.2",
    "postcss": "^8.5.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "publint": "^0.2.12",
    "svelte": "^5.42.2",
    "svelte-check": "^4.3.3",
    "tailwindcss": "^3.4.18",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.2",
    "vite": "^5.4.21",
    "vitest": "^2.1.9"
  },
  "peerDependenciesMeta": {
    "@deck.gl/core": {
      "optional": true
    },
    "@deck.gl/layers": {
      "optional": true
    },
    "@deck.gl/mapbox": {
      "optional": true
    }
  },
  "packageManager": "pnpm@9.6.0+sha512.38dc6fba8dba35b39340b9700112c2fe1e12f10b17134715a4aa98ccf7bb035e76fd981cf0bb384dfa98f8d6af5481c2bef2f4266a24bfa20c34eb7147ce0b5e"
}
